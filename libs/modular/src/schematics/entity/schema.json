{
  "$schema": "http://json-schema.org/schema",
  "id": "entity-options",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Entity name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "domain": {
      "type": "string",
      "description": "Domain name"
    }
  },
  "required": ["name", "domain"]
}
